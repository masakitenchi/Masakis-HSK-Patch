<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Save Our Ship 2</li>
	</mods>
		<match Class="PatchOperationSequence">
			<operations>
		
				<!-- ========== Recipes(Seed Extraction) ========== -->
				
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
						  <defName>ExtractSeed_Vanoroot</defName>
						  <label>extract vanoroot seeds</label>
						  <description>Extract seeds from Vanoroot</description>
						  <ingredients>
							<li>
							  <filter>
								<thingDefs>
								  <li>RawVanoroot</li>
								</thingDefs>
							  </filter>
							  <count>4</count>
							</li>
						  </ingredients>
						  <fixedIngredientFilter>
							<thingDefs>
							  <li>RawVanoroot</li>
							</thingDefs>
						  </fixedIngredientFilter>
						  <products>
							<Seed_Vanoroot>3</Seed_Vanoroot>
						  </products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
						  <defName>ExtractSeed_Pharma</defName>
						  <label>extract pharma seeds</label>
						  <description>Extract seeds from Neutroamine</description>
						  <ingredients>
							<li>
							  <filter>
								<thingDefs>
								  <li>Neutroamine</li>
								</thingDefs>
							  </filter>
							  <count>4</count>
							</li>
						  </ingredients>
						  <fixedIngredientFilter>
							<thingDefs>
							  <li>Neutroamine</li>
							</thingDefs>
						  </fixedIngredientFilter>
						  <products>
							<Seed_Pharma>3</Seed_Pharma>
						  </products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
						  <defName>ExtractSeed_AmbrosiaArchotech</defName>
						  <label>extract ambrosiaarchotech seeds</label>
						  <description>Extract seeds from Ambrosia</description>
						  <ingredients>
							<li>
							  <filter>
								<thingDefs>
								  <li>Ambrosia</li>
								</thingDefs>
							  </filter>
							  <count>4</count>
							</li>
						  </ingredients>
						  <fixedIngredientFilter>
							<thingDefs>
							  <li>Ambrosia</li>
							</thingDefs>
						  </fixedIngredientFilter>
						  <products>
							<Seed_AmbrosiaArchotech>3</Seed_AmbrosiaArchotech>
						  </products>
						</RecipeDef>
					</value>
				</li>
				
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<RecipeDef ParentName="ExtractSeed">
							<defName>ExtractSeed_Meatbush</defName>
							<label>extract Meatbush seeds</label>
							<description>Extract seeds from Meatbush</description>
							<ingredients>
								<li>
									<filter>
										<thingDefs>
											<li>Meat_Muffalo</li>
										</thingDefs>
									</filter>
									<count>4</count>
								</li>
							</ingredients>
							<fixedIngredientFilter>
								<thingDefs>
									<li>Meat_Muffalo</li>
								</thingDefs>
							</fixedIngredientFilter>
							<products>
								<Seed_Meatbush>3</Seed_Meatbush>
							</products>
						</RecipeDef>
					</value>
				</li>
				
			</operations>
		</match>
    </Operation>
	
</Patch>