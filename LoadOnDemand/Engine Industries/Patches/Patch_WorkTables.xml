<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[@ParentName="WorkTable" and comps/li[@Class="CompProperties_Power"]]/comps</xpath>
		<value>
			<li Class="Core_SK_Patch.CompProperties_Engine">
				<EnginesCanUse>
					<li>EngineLight</li>
					<li>EngineSteam</li>
					<li>EngineMedium</li>
					<li>EngineHeavy</li>
					<li>EngineTurbine</li>
					<li>EngineJet</li>
				</EnginesCanUse>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineLight"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>1.05</EngineEfficiency>
				<PowerConsumptionMultiplier>1.2</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineSteam"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>1.1</EngineEfficiency>
				<PowerConsumptionMultiplier>1.3</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineMedium"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>1.3</EngineEfficiency>
				<PowerConsumptionMultiplier>1.6</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineHeavy"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>1.7</EngineEfficiency>
				<PowerConsumptionMultiplier>2.0</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineTurbine"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>1.8</EngineEfficiency>
				<PowerConsumptionMultiplier>2.2</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
	<Operation Class="PatchOperationAddModExtension">
		<xpath>Defs/ThingDef[defName="EngineJet"]</xpath>
		<value>
			<li Class="Core_SK_Patch.EngineModExtension">
				<EngineEfficiency>2.0</EngineEfficiency>
				<PowerConsumptionMultiplier>2.4</PowerConsumptionMultiplier>
			</li>
		</value>
	</Operation>
</Patch>