<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<RecipeDef>
		<defName>CSP_ConvertToWolframite</defName>
		<label>Convert to wolframite ore</label>
		<description>Convert inorganic matter into wolframite, the ore of tungsten. Produces 10.</description>
		<jobString>Converting matter into wolframite ore.</jobString>
		<workSpeedStat>ElectronicCraftingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<workAmount>12000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Matter</li>
					</thingDefs>
				</filter>
				<count>250</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Matter</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Wolframite>10</Wolframite>
		</products>
		<skillRequirements>
			<Crafting>15</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Super_matter_E1</researchPrerequisite>
	</RecipeDef>

</Defs>